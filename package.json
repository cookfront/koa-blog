{
  "name": "koa-blog",
  "version": "0.0.1",
  "description": "koa blog",
  "main": "index.js",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-build",
    "start": "npm-run-all --parallel serve open:src lint:watch",
    "serve": "nodemon app.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:build": "babel-node tools/buildServer.js",
    "lint": "esw webpack.config.* public/src tools controllers models services",
    "lint:watch": "npm run lint -- --watch",
    "clean-build": "npm run remove-build && mkdir -p public/build",
    "remove-build": "rimraf public/build",
    "autoprefixer": "postcss -u autoprefixer -r public/build/css/*",
    "scss": "node-sass --output-style compressed -o public/build/css public/src/scss",
    "watch:css": "onchange 'public/src/scss/*.scss' -- npm run build:css",
    "build:css": "npm run scss && npm run autoprefixer",
    "prebuild": "npm run clean-build && npm run lint",
    "build": "babel-node tools/build.js && npm run open:build",
    "watch": ""
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "browser-sync": "^2.12.12",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.2.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-watch": "^2.1.11",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.19.0",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.1.1",
    "onchange": "^2.4.0",
    "postcss-cli": "^2.5.2",
    "qiniu": "^6.1.11",
    "react-addons-test-utils": "^15.1.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "author": "cookfront",
  "license": "MIT",
  "dependencies": {
    "config": "^1.21.0",
    "koa": "^1.2.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "sequelize": "^3.23.3"
  }
}
